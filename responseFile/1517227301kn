    isMobile();
    function isMobile(){
        if(navigator.userAgent.match(/(iPhone|iPod|Android|ios)/i)!=null){
            var url = location.search;
            if (url.indexOf("itemId") != -1){
                location.href="//m.i360mall.com/item.html"+url;
            } else {
                location.href="//m.i360mall.com/";
            }
        }
    }
