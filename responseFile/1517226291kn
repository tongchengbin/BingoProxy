                        function setWideImgs(imgList) {
                            for (var i = 0; i < imgList.length; i++) {
                                var jqDom = $(imgList[i]),
                                        allUrl = jqDom.css("background-image");
                                var srcI = allUrl.substring(4, allUrl.length - 1);
                                srcI = srcI.replace(/(\"|\')/g, "");
