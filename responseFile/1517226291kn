<script>
    var limit_num = 200;
    var stock_num = 200;
    $('.tcode-w .tcode').mouseenter(function () {
        $('.tcode-w').addClass('tcodeHover');
    }).mouseleave(function () {
        $('.tcode-w').removeClass('tcodeHover');
    })
    $('.tcode-d .tcode').mouseenter(function () {
        $('.tcode-d').addClass('tcodeHover');
    }).mouseleave(function () {
        $('.tcode-d').removeClass('tcodeHover');
    })

    var dealWithBtns = function (value) {
        value = parseInt($.trim(value), 10);
        var max = limit_num;
        var storeNum = stock_num;

        if (max > 0 && value >= max) {
            $(".gcIpt .increment").addClass("disabled");
        }
        else if (value >= storeNum) {
            $(".gcIpt .increment").addClass("disabled");
        }
        else {
            $(".gcIpt .increment").removeClass("disabled");
        }

        if (value > 1) {
            $(".gcIpt .decrement").removeClass("disabled");
        }
        else {
            $(".gcIpt .decrement").addClass("disabled");
        }
    };

    $(".goodsCount").bind("setvalue", function (e) {
        dealWithBtns($(this).val());
    });

    $(".goodsCount").bind("textblur", function (e) {
        var tar = $(e.target);
        var text = $.trim($(tar).val());

        if ("" === text) {
            $(tar).val("1");
            $(tar).trigger("setvalue");
            return;
        }

        if (!/^\d+$/.test(text)) {
            $(tar).val("1");
            $(tar).trigger("setvalue");
            return;
        }

        var val = parseInt(text, 10);
        var max = limit_num | 0;
        var storeNum = parseInt(stock_num, 10);

        if (val === 0) {
            $(tar).val("1");
            $(tar).trigger("setvalue");
            return;
        }

        if (max > 0 && val > max) {
            mall.dialog.alert("购买数量超出限制!");
            $(tar).val(max);

            $(tar).trigger("setvalue");
            return;
        }

        if (max == 0 && val > storeNum) {
            mall.dialog.alert("购买数量超出库存!");
            $(tar).val(storeNum);

            $(tar).trigger("setvalue");
            return;
        }

        $(tar).trigger("setvalue");
    });

    $(".goodsCount").on("blur", function () {
        $(this).trigger("textblur");
    });

    // {%if $is_main==0 && $main_item['store_num']!=0%}
    $(".decrement,.increment").click(function (e) {
        var count = $(this).siblings(".goodsCount").val() | 0;
        var num = $(this).attr('data-num') | 0;
        var max = limit_num | 0;

        count = count + num;

        if (count < 1) {
            count = 1;
            $(".decrement").addClass("disabled");
        }
        else if (max > 0 && count > max) {
            mall.dialog.alert("已达到最大购买数量，无法继续添加");
            $(".increment").addClass("disabled");
            count = max;
        }
        else {
            $(".increment").removeClass("disabled");
            $(".decrement").removeClass("disabled");
        }

        $(this).siblings(".goodsCount").val(count);
        $(".goodsCount").trigger("setvalue");
    });

    $(".decrement,.increment").hover(function (e) {
        var eobj = $(e.target);
        if (!eobj.hasClass("disabled")) {
            eobj.addClass("hover");
        }
    }, function (e) {
        $(e.target).removeClass("hover");
    });
    $(".getCart,.gInfoBtn-addcart").click(function (e) {
        mall.header.getUserInfo({},function () {
            $.post("/cart/addToCart", {
                itemId: "4021675",
                num: $(".goodsCount").val()
            }, function (dt) {
                if (dt.errorCode == 0) {
                    goto("//trade.i360mall.com/cart?t=" + new Date().getTime());
                }else if (dt.errorCode == 201) {
                    mall.dialog.alert("加入购物车太频繁，请稍后再试");
                } else {
                    mall.dialog.alert("加入购物车失败");
                }
            }, 'json');
        },function(){
            location.href = "/login?returnUrl="+encodeURIComponent("//i360mall.com/shop/item?itemId=4021675&t="+(new Date()).getTime());
        });
        log&&log('360PC_201709197','67','4021675');
    });
    function bespokeOrNot(){
        mall.header.getUserInfo({},function () {
            $.post("/shop/bespokeOrNot", {
                itemId: "4021675"
            }, function (data) {
                if (data ) {
                    addToCart();
                }else {
                    location.href = "/shop/noBespeak?itemId=" + "4021675"+"&t=" + new Date().getTime();
                }
            }, 'json');
        },function(){
            location.href = "/login?returnUrl="+encodeURIComponent("//i360mall.com/shop/item?itemId=4021675&t="+(new Date()).getTime());
        });
    }
    function changePage (itemId,type,num,current,pageSize,total) {
        var pagenum;
        if(type == 'first'){
            pagenum = 1;
        }else if(type == 'last'){
            pagenum = total;
        } else if(type == 'up'){
            pagenum = current-1;
        }else if(type == 'down'){
            pagenum = current + 1;
        }else if(type == 'x'){
            pagenum = num;
        }
        $(".eval-list").empty();
        $(".pagelist").remove();
        shopItem.getComment(itemId,pagenum,pageSize);
        var top = $(".eval-list").offset().top;
        $('body,html').animate({scrollTop:top}, 500);
    }
    $("#bespeakNow").click(function () {
                $(this).removeAttr("onclick");
                location.href = "/shop/addPresellQuali?itemId=" + "4021675"+"&t=" + new Date().getTime();
            }
    );
    $("#bespeakNow-float").click(function () {
                $(this).removeAttr("onclick");
                location.href = "/shop/addPresellQuali?itemId="+ "4021675"+"&t=" + new Date().getTime();
            }
    );
    var addCartFlag = true;
    function addToCart(){
        $("#buyNow").removeAttr("onclick");
        $("#buyNow-float").removeAttr("onclick");
        mall.header.getUserInfo({},function () {
            $.post("/cart/addToCart", {
                itemId: "4021675",
                num: $(".goodsCount").val()
            }, function (dt) {
                if (dt.errorCode == 0) {
                    goto("//trade.i360mall.com/cart?t=" + new Date().getTime());
                }else if (dt.errorCode == 201) {
                    mall.dialog.alert("加入购物车太频繁，请稍后再试");
                } else {
                    mall.dialog.alert("加入购物车失败");
                }
            }, 'json');
        },function(){
            location.href = "/login?returnUrl="+encodeURIComponent("//i360mall.com/shop/item?itemId=4021675&t="+(new Date()).getTime());
        });
    }

    function SecKill(){
        goto("//trade.i360mall.com/seckill/gotoSecKill?itemId=" + "4021675"+"&t=" + new Date().getTime());
        $(this).removeAttr("onclick");

    }
</script>

