    var bindArea = function(){
        $.ajax({
            url: "/area/getByLocationId",
            type: "post",
            async:true,
            data:{"locationId":getCookie('_area_id_')},
            dataType: "json",
            success: function(data){
                if(data && data.errorCode == 0 && data.data){
                    var locationId = data.data;
                    //$(".m-address-tt").html(location['name'] + '<i></i>');
                    $(".m-address-tt").html(area.getLocation(locationId['code']) + '<i></i>');

                    $(".m-address-tt").on('click', function(e) {
                        if($(".m-address")&&$(".m-address").length==0){
                            area.init("4021675",locationId['code']);
                            $('.tt-item.level-2').trigger("click");
                        }
                        var _this = $(this).parent();
                        _this.toggleClass('open');
                    });
                }
            },
            error : function() {
                //init(cateData);
            }
        });

        $(body).click(function(e){
            if(!$(e.target).is('.g-address') && !$(e.target).is('.g-address *')){
                $('.g-address').removeClass('open');
            }
        });
    };

    function getCookie(c_name)
    {
        if (document.cookie.length>0)
        {
            c_start=document.cookie.indexOf(c_name + "=");
            if (c_start!=-1)
            {
                c_start=c_start + c_name.length+1;
                c_end=document.cookie.indexOf(";",c_start);
                if (c_end==-1) c_end=document.cookie.length;
                return unescape(document.cookie.substring(c_start,c_end));
            }
        }
        return "";
    };
    var initService = function(){
        if("4021675"!=""){
            $(".inline-kefu").attr("onclick","clickService()");
            $(".chatjd_dl").attr("onclick","clickService()");
        }
    };

    var clickService = function(){
        window.open("/chat/forItem?itemId=4021675&t=" + (new Date()).getTime());
        return true;
    };
</script>

